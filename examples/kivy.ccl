kivy := ImportKivy();

app := {
    self := kivy.app();
    self.build = \ {
        container := kivy.uix.boxLayout();
        container.addWidget(kivy.uix.label('hello world!'));
        container.addWidget(kivy.uix.label('hello again!'));
        container;
    };
    self;
};

app.setTitle('new title!');

app.run();
